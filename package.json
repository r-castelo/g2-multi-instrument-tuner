{
  "name": "g2-guitar-bass-ukulele-tuner",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "description": "Multi-instrument tuner for Even Realities G2 smart glasses",
  "scripts": {
    "dev": "vite",
    "build": "tsc --noEmit && vite build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "test": "tsx --test tests/**/*.test.ts",
    "sim": "evenhub-simulator http://localhost:5173",
    "qr": "evenhub qr",
    "pack": "npm run build && evenhub pack app.json ./dist --output g2-multi-tuner.ehpk"
  },
  "dependencies": {
    "@evenrealities/even_hub_sdk": "^0.0.7",
    "@evenrealities/evenhub-cli": "^0.1.5",
    "@evenrealities/evenhub-simulator": "^0.4.1"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "tsx": "^4.19.0",
    "typescript": "^5.7.0",
    "vite": "^6.0.0"
  }
}
